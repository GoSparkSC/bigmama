.bar
    width 100%
    height 1px
    background-color black

.event-container
    z-index 2
    position absolute
    width 100%
    padding 0 5px
    top 5px


.event-inner-container
    position relative
    width 100%
    background-color white

.event-content-container
    width 80%
    margin-left 10%
	
.single-event-image
    width 100%
    height 500px

    backgroud-position center
    background-size cover
    background-repeat no-repeat
    background-image url("http://i2.wp.com/viterbiconnect.usc.edu/wp-content/uploads/2015/02/startup.png?resize=969%2C529")
    
.member-photo
    width 60px
    height 60px
    -webkit-border-radius: 150px;
    -moz-border-radius: 150px;
    border-radius 30px
    margin 10px
    
    backgroud-position center
    background-size cover
    background-repeat no-repeat
    background-image url("https://media.licdn.com/mpr/mpr/shrinknp_400_400/p/5/005/053/334/18b66a7.jpg")

.sparksc-letterhead
    margin-top 10px
    width 150px
